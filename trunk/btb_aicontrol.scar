			end
			
			if topRightOK then
				listk = listk+1
				tempH = math.abs((curbase.x+1)-tx) + math.abs((curbase.y-1)-ty)
				table.insert(openlist, listk, {x=curbase.x+1, y=curbase.y-1, g=tempG, h=tempH, f=tempG+tempH, par=closedk})
			end
			if topLeftOK then
				listk = listk+1
				tempH = math.abs((curbase.x-1)-tx) + math.abs((curbase.y-1)-ty)
				table.insert(openlist, listk, {x=curbase.x-1, y=curbase.y-1, g=tempG, h=tempH, f=tempG+tempH, par=closedk})
			end
			if bottomRightOK then
				listk = listk+1
				tempH = math.abs((curbase.x+1)-tx) + math.abs((curbase.y+1)-ty)
				table.insert(openlist, listk, {x=curbase.x+1, y=curbase.y+1, g=tempG, h=tempH, f=tempG+tempH, par=closedk})
			end
			if bottomLeftOK then
				listk = listk+1
				tempH = math.abs((curbase.x-1)-tx) + math.abs((curbase.y+1)-ty)
				table.insert(openlist, listk, {x=curbase.x-1, y=curbase.y+1, g=tempG, h=tempH, f=tempG+tempH, par=closedk})
