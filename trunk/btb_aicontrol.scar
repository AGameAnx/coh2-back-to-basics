			end
			
			-- Add points to openlist
			if rightOK then
				listk = listk+1
				tempH = math.abs((curbase.x+1)-tx) + math.abs(curbase.y-ty)
				table.insert(openlist, listk, {x=curbase.x+1, y=curbase.y, g=tempG, h=tempH, f=tempG+tempH, par=closedk})
